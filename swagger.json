{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "Meme API Backend",
    "description": "A simple meme creator and streamer application built on NodeJS."
  },
  "host": "localhost:8000",
  "basePath": "/",
  "tags": [
    {
      "name": "Memes",
      "description": "API for different GET, POST request for memes in the system"
    }
  ],
  "consumes": ["application/json"],
  "produces": ["application/json"],
  "paths": {
    "/memes": {
      "post": {
        "tags": ["Memes"],
        "description": "Create a meme post.",
        "parameters": [
          {
            "name": "meme",
            "in": "body",
            "schema": {
              "$ref": "#/definitions/MemeModel"
            }
          }
        ],
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "Response",
            "schema": {
              "$ref": "#/definitions/MemeResponse"
            }
          }
        }
      },
      "get": {
        "tags": ["Memes"],
        "description": "Get list of memes upto 100.",
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "Response",
            "schema": {
              "$ref": "#/definitions/MemeResponse"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "MemeModel": {
      "required": ["name", "caption", "url"],
      "properties": {
        "name": {
          "type": "string"
        },
        "caption": {
          "type": "string"
        },
        "url": {
          "type": "string"
        }
      }
    },
    "MemeResponse": {
      "required": ["name", "_id", "caption", "url"],
      "properties": {
        "_id": {
          "type": "number"
        },
        "name": {
          "type": "string"
        },
        "caption": {
          "type": "string"
        },
        "url": {
          "type": "string"
        }
      }
    },
    "MemeGET": {
      "type": "array"
    }
  }
}
